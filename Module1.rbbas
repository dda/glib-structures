#tag ModuleProtected Module Module1	#tag Method, Flags = &h0		Function SortFunc2(a As MemoryBlock, b As MemoryBlock) As Integer		  MsgBox ":"		  'gint (*GCompareFunc) (gconstpointer a, gconstpointer b);		  // Returns: negative value if a < b; zero if a = b; positive value if a > b.		  		  If a.StringValue(0, 4) < b.StringValue(0, 4) Then Return -1		  If a.StringValue(0, 4) = b.StringValue(0, 4) Then Return 0		  If a.StringValue(0, 4) > b.StringValue(0, 4) Then Return 1		  		  		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function SortFunc3(a As MemoryBlock, b As MemoryBlock, userData As MemoryBlock) As Integer		  MsgBox ":"		  'gint (*GCompareFunc) (gconstpointer a, gconstpointer b);		  // Returns: negative value if a < b; zero if a = b; positive value if a > b.Ptr(0).		  		  Dim mySize As Integer		  mySize=userData.Ptr(0).UInt32Value(0)		  		  If a.Ptr(0).StringValue(0, mySize) < b.Ptr(0).StringValue(0, mySize) Then Return -1		  If a.Ptr(0).StringValue(0, mySize) = b.Ptr(0).StringValue(0, mySize) Then Return 0		  If a.Ptr(0).StringValue(0, mySize) > b.Ptr(0).StringValue(0, mySize) Then Return 1		  		  		End Function	#tag EndMethod	#tag ViewBehavior		#tag ViewProperty			Name="Index"			Visible=true			Group="ID"			InitialValue="-2147483648"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Left"			Visible=true			Group="Position"			InitialValue="0"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Name"			Visible=true			Group="ID"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Super"			Visible=true			Group="ID"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Top"			Visible=true			Group="Position"			InitialValue="0"			InheritedFrom="Object"		#tag EndViewProperty	#tag EndViewBehaviorEnd Module#tag EndModule